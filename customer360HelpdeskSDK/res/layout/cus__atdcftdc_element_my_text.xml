<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cus_atdcftdcemt_ll_attachmentWrapper"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
	
     <!-- <RelativeLayout
        android:id="@+id/liHolder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left" > -->
    <RelativeLayout
        android:id="@+id/cus_atdcftdcemt_ll_contentWrapper"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="90dp" >

        <LinearLayout
            android:id="@+id/cus_atdcftdcemt_ll_contentWrapper2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/activity_margin_default"
            android:layout_marginTop="@dimen/cus_atdcftdcemt_ll_contentWrapper2_layout_margin"
            android:layout_marginRight="16dp"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:paddingLeft="16dp"
            android:minWidth="120dp"
            android:paddingBottom="@dimen/cus_atdcftdcemt_ll_contentWrapper2_padding"
            android:paddingRight="@dimen/cus_atdcftdcemt_ll_contentWrapper2_padding"
            android:paddingTop="@dimen/cus_atdcftdcemt_ll_contentWrapper2_padding" >

            <!-- <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="10dp"
                android:background="#C8C8C8"
                android:visibility="gone" /> -->

            <LinearLayout
                android:id="@+id/cus_atdcftdcemt_ll_contentWrapper4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/cus_atdcftdcemt_iv_Icon"
                    android:layout_width="@dimen/cus_atdcftdcemt_iv_Icon_layout_width"
                    android:layout_height="@dimen/cus_atdcftdcemt_iv_Icon_layout_height"
                    android:layout_marginRight="@dimen/cus_atdcftdcemt_iv_Icon_layout_marginRight"
                    android:layout_marginLeft="4dp"
                    android:visibility="gone"
                    android:src="@drawable/circle_agent" /> 

                <LinearLayout
                    android:id="@+id/cus_atdcftdcemt_ll_contentWrapper3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/cus_atdcftdcemt_tv_Name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Miley Cyrus"
                        android:textColor="#000"
                        android:alpha="0.45"
                        android:visibility="gone"
                        android:textSize="@dimen/default_text_size_plus_2" />

                    <TextView
                        android:id="@+id/cus_atdcftdcemt_tv_Text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="sample Ticket Title text"
                        android:textColor="#666666"
                        android:textSize="@dimen/default_text_size_plus_2" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#C8C8C8"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/cus_atdcftdcemt_ll_PlaceHolerAttachments"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/cus_round_shape_default"
                android:gravity="center_vertical"
                android:orientation="vertical"
                android:visibility="gone" >

                <include layout="@layout/cus__atdcftdc_element_attachment" />
            </LinearLayout>
        </LinearLayout>

        <!-- <View
            android:id="@+id/cus_atdcftdcemt_v_emptyspace"
            android:layout_width="@dimen/cus_atdcftdcemt_v_emptyspace_layout_width"
            android:layout_height="match_parent"
            android:visibility="visible" /> -->
            
             <TextView
	        android:id="@+id/txtTime"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text=""
	        android:textColor="#c7c7c7"
	        android:textSize="@dimen/default_text_size_minus_2"
	        android:layout_below="@+id/cus_atdcftdcemt_ll_contentWrapper2"
        	android:layout_alignRight="@+id/cus_atdcftdcemt_ll_contentWrapper2"
        	android:paddingLeft="16dp"/>
             
    </RelativeLayout>
    
   
   <!--  </RelativeLayout> -->

</LinearLayout>